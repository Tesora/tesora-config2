cache:
  expiration:
    server: 5
    port: 5
    floating-ip: 5
clouds:
  tesorarackspace:
    private: true
    region-name: 'RegionOne'
    auth:
      username: '<%= @tesora_rackspace_username %>'
      password: '<%= @tesora_rackspace_password %>'
      project_name: '<%= @tesora_rackspace_project %>'
      auth_url: 'https://cloud.elasticdb.org:5000/v3'
      domain_name: 'Default'
    identity_api_version: '3'
    image_format: 'qcow2'
    use_internal_network: true
    use_external_network: false
    force_ivp4: true
